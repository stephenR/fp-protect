  test $ac_status = 0; }; }; then :
  libc_cv_cc_sse4=yes
else
  libc_cv_cc_sse4=no
fi

fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $libc_cv_cc_sse4" >&5
$as_echo "$libc_cv_cc_sse4" >&6; }
if test $libc_cv_cc_sse4 = yes; then
  $as_echo "#define HAVE_SSE4_SUPPORT 1" >>confdefs.h

fi

